{
  "services": {
    "stirling-pdf": {
      "name": "Stirling-PDF",
      "image": "stirlingtools/stirling-pdf",
      "envVars": {
        "DOCKER_ENABLE_SECURITY": "true",
        "LANGS": "en_US,es-mx,fa",
        "OCR_ENABLED": "true",
        "ENABLE_EXTERNAL_DB": "true",
        "ENABLE_FOLDER_SCAN": "true",
        "SIGNING_ENABLED": "true",
        "DATABASE_URL": "${{DATABASE_URL}}"
      },
      "volumes": {
        "trainingData": "/usr/share/tesseract-ocr/4.00/tessdata",
        "extraConfigs": "/configs",
        "customFiles": "/customFiles",
        "logs": "/logs",
        "pipeline": "/pipeline",
        "storage": "/app/storage",
        "input": "/app/input",
        "output": "/app/output"
      },
      "ports": {
        "8080": {
          "protocol": "http"
        }
      }
    }
  }
}
